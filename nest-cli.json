{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/external-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/external-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/external-api",
  "projects": {
    "external-api": {
      "type": "application",
      "root": "apps/external-api",
      "entryFile": "main",
      "sourceRoot": "apps/external-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/external-api/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "sqs": {
      "type": "library",
      "root": "libs/sqs",
      "entryFile": "index",
      "sourceRoot": "libs/sqs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sqs/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "auth-code-send-worker": {
      "type": "application",
      "root": "apps/auth-code-send-worker",
      "entryFile": "main",
      "sourceRoot": "apps/auth-code-send-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-code-send-worker/tsconfig.app.json"
      }
    },
    "mailer": {
      "type": "library",
      "root": "libs/mailer",
      "entryFile": "index",
      "sourceRoot": "libs/mailer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mailer/tsconfig.lib.json"
      }
    }
  }
}